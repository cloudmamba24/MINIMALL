{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"linter": {
		"enabled": true,
		"rules": {
			"suspicious": {
				"noExplicitAny": "warn"
			},
			"style": {
				"noNonNullAssertion": "off"
			},
			"correctness": {
				"noUnusedVariables": "off"
			}
		}
	},
	"formatter": {
		"enabled": true
	}
}